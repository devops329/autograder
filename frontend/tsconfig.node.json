    "skipLibCheck": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "esModuleInterop": true,
    "outDir": "dist",
    "target": "ES2020",
    "rootDir": "service"
  },
  "include": ["service/**/*.ts"]
}
